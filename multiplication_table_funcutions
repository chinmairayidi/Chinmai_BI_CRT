#write the python program to bulid a funcution which prints the multiplication table of n 
def print_multiplication_table(num):
    print(f"Multiplication Table of {num}:")
    for i in range(1, 11):
        print(f"{num} x {i} = {num * i}")
number = int(input("Enter a number: "))
print_multiplication_table(number)


#write the python program to bulid a funcution which prints the multiplication table of 1 to n
def print_multi_table(n_1):
    print(f"table of {n_1}:")
    for i in range(1,n_1):
        print(f"{n_1} * {i} = {n_1 * i}") 
n_1 = input()
print_multi_table(n_1)